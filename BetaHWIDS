local wl = "0879ae585b497b3105e39a26327210450fc975b7a720a19b0cfa8ab2445516a368027de21a40620d412246ab6fdbb579f62d59c0e47d44c54ac6b657d674e7a0"
local Characters =
    "😀😃😁😅😊😇🙂🙃😉😌😍🥰😘😙😚😋😛😝😜🤪🤨🧐🤓️😣😖😫😩🥺😢😭😤😠😡🤬🤯😳🥵🥶😱😨😰😥😓🤗🤔🤭🤫🤥😶😐😑😬🙄😯😦😧😮😲🥱😴🤤😪😵🤐🥴🤢🤮🤧😷🤒🤕👹👺🤡💩👻💀☠🎃😺😸😹😻😼😽🙀😿😾會帶走小雞的陰戶我是你的死性交是的酷爸爸абвгдеёжхийклмнпрстуфхцчшщъыьэюя萩敦媛嵐椎翔喬巽湧椅喉腎痩貼斑喩勤"
local tChars = {}
for str in Characters:gmatch("[^.-*]") do
    table.insert(tChars, str)
end
Characters = tChars
tChars = nil

local function GetRandomCharacter(IgnoreCharacter, UL)
    local Char = Characters[math.random(1, #Characters)]

    if IgnoreCharacter then
        while Char == IgnoreCharacter do
            Char = Characters[math.random(1, #Characters)]
        end
    end

    if UL then
        Char = (math.random(0, 1) == 0) and Char:lower() or Char:upper()
    end

    return Char
end

local function GenerateString(Length)
    local String = {}
    local Recent
    for i = 1, Length do
        local char = GetRandomCharacter(Recent, true)
        table.insert(String, char)
        Recent = char
    end

    return table.concat(String, "")
end
if hwid == wl then
print(GenerateString(1000))
else
setclipboard(GenerateString(1000))
game:Shutdown()
end
